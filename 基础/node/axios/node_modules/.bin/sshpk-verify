#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../_sshpk@1.17.0@sshpk/bin/sshpk-verify" "$@"
else 
  exec node  "$basedir/../_sshpk@1.17.0@sshpk/bin/sshpk-verify" "$@"
fi
