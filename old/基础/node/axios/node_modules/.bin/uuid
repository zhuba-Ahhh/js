#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../_uuid@3.4.0@uuid/bin/uuid" "$@"
else 
  exec node  "$basedir/../_uuid@3.4.0@uuid/bin/uuid" "$@"
fi
